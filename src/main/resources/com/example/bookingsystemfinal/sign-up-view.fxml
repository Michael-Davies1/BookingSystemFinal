<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bookingsystemfinal.SignUpController">
    <children>
        <PasswordField fx:id="ConfirmPassword" layoutX="76.0" layoutY="184.0" promptText="Confirm Password" />
        <PasswordField fx:id="signUpPassword" layoutX="76.0" layoutY="150.0" promptText="Password" />
        <TextField fx:id="signUpEmailtxt" layoutX="76.0" layoutY="43.0" promptText="Email-Address" />
        <TextField fx:id="ConfirmEmailtxt" layoutX="76.0" layoutY="80.0" promptText="Confirm Email-Address" />
        <Button fx:id="SignUpbtn" layoutX="124.0" layoutY="246.0" mnemonicParsing="false" onAction="#signUpbtn" text="Sign-Up" />
        <Label fx:id="SignUpStatus" layoutX="11.0" layoutY="7.0" prefHeight="32.0" prefWidth="259.0" text="Status" textFill="#f80e0e" />
    </children>
</AnchorPane>
